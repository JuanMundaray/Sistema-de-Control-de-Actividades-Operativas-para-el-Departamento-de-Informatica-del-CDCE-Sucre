CREATE TABLE IF NOT EXISTS actividades.usuario (id_usuario serial, nombre_usuario character varying(100)  NOT NULL, nombre_personal character varying(100)  NOT NULL, cedula character varying(10)  NOT NULL, contrasena character varying(100)  NOT NULL, tipo_usuario character varying(100)  NOT NULL, fecha_creacion date NOT NULL, departamento_usuario integer NOT NULL, apellido_personal character varying(100) NOT NULL,
marca_existencia boolean NOT NULL, CONSTRAINT usuario_pkey PRIMARY KEY (id_usuario), CONSTRAINT cedulauni UNIQUE (cedula), CONSTRAINT nombre_usuariouni UNIQUE (nombre_usuario), CONSTRAINT usuario_id_departamento_fkey FOREIGN KEY (departamento_usuario) REFERENCES actividades.departamentos (id_departamento) MATCH SIMPLE ON UPDATE NO ACTION ON DELETE NO ACTION);INSERT INTO actividades.usuario(nombre_usuario, nombre_personal, cedula, contrasena, tipo_usuario, fecha_creacion, departamento_usuario, apellido_personal, marca_existencia) VALUES ('demo','CARLOS JONAS',123456789,'1234','administrador','2024-03-03',1,'MATA SALAZAR',true);